{
    "name": "back-ng",
    "version": "1.1.0",
    "description": "Dynamic back navigation supporting web history within angular",
    "author": "Andrew Brunker <andrew@brunker.au>",
    "license": "Apache-2.0",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ElCapitanSponge/BackNG.git"
    },
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build",
        "watch": "ng build --watch --configuration development",
        "test": "ng test",
        "package": "npm pack ./dist --pack-destination releases",
        "docs": "compodoc -p tsconfig.doc.json -n \"BackNG Documentation\" -d ./docs --disableSourceCode --disableTemplateTab --disableStyleTab --disableDomTree --hideGenerator --disableCoverage",
        "docs:development": "compodoc -p tsconfig.doc.json -n \"BackNG Documentation\" -d ./dev-docs",
        "changelog": "changelog generate"
    },
    "dependencies": {
        "@angular/animations": "^16.0.4",
        "@angular/common": "^16.0.4",
        "@angular/compiler": "^16.0.4",
        "@angular/core": "^16.0.4",
        "@angular/forms": "^16.0.4",
        "@angular/platform-browser": "^16.0.4",
        "@angular/platform-browser-dynamic": "^16.0.4",
        "@angular/router": "^16.0.4",
        "rxjs": "~7.8.1",
        "tslib": "^2.5.3",
        "zone.js": "~0.13.0"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^16.0.5",
        "@angular/cli": "~16.0.5",
        "@angular/compiler-cli": "^16.0.4",
        "@compodoc/compodoc": "^1.1.21",
        "@types/jasmine": "~4.3.2",
        "generate-changelog": "^1.8.0",
        "jasmine-core": "~5.0.0",
        "karma": "~6.4.2",
        "karma-chrome-launcher": "~3.2.0",
        "karma-coverage": "~2.2.0",
        "karma-jasmine": "~5.1.0",
        "karma-jasmine-html-reporter": "~2.1.0",
        "ng-packagr": "^16.0.1",
        "typescript": "~4.9.4"
    }
}
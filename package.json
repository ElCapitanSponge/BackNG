{
  "name": "back-ng",
  "version": "1.2.0",
  "description": "Dynamic back navigation supporting web history within angular",
  "author": "Andrew Brunker <andrew@brunker.au>",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ElCapitanSponge/BackNG.git"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "package": "npm pack ./dist --pack-destination releases",
    "docs": "compodoc -p tsconfig.doc.json -n \"BackNG Documentation\" -d ./docs --disableSourceCode --disableTemplateTab --disableStyleTab --disableDomTree --hideGenerator --disableCoverage",
    "docs:development": "compodoc -p tsconfig.doc.json -n \"BackNG Documentation\" -d ./dev-docs",
    "changelog": "changelog generate"
  },
  "dependencies": {
    "@angular/animations": "^17.0.1",
    "@angular/common": "^17.0.1",
    "@angular/compiler": "^17.0.1",
    "@angular/core": "^17.0.1",
    "@angular/forms": "^17.0.1",
    "@angular/platform-browser": "^17.0.1",
    "@angular/platform-browser-dynamic": "^17.0.1",
    "@angular/router": "^17.0.1",
    "rxjs": "~7.8.1",
    "tslib": "^2.5.3",
    "zone.js": "~0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.0.0",
    "@angular/cli": "~17.0.0",
    "@angular/compiler-cli": "^17.0.1",
    "@compodoc/compodoc": "^1.1.21",
    "@types/jasmine": "~4.3.2",
    "generate-changelog": "^1.8.0",
    "jasmine-core": "~5.0.0",
    "karma": "~6.4.2",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^17.0.0",
    "typescript": "~5.2.2"
  }
}
